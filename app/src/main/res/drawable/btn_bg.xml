<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#FFC107" />
            <corners android:radius="50dp" />
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#EA3F65" />
            <corners android:bottomLeftRadius="50dp" android:topRightRadius="59dp" />
            <stroke android:width="1dp" android:color="#FFFFFF" />
            <padding android:bottom="8dp" android:left="16dp" android:right="16dp" android:top="8dp" />
            <size android:width="160dp" android:height="40dp" />
        </shape>
    </item>
</selector>